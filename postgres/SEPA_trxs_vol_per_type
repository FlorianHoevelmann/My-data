Select sum(amount_cents/100.0), count(created) from dwh_adorable_transaction 
left join dwh_country_codes on dwh_country_codes.id = dwh_adorable_transaction.merchant_country 
where dwh_country_codes.two_letter in ('BE',
'BG',
'DK',
'DE',
'EE',
'FI',
'FR',
'GR',
'GB',
'IE',
'IS',
'IT',
'HR',
'LV',
'LI',
'LT',
'LU',
'MT',
'MC',
'NL',
'NO',
'AT',
'PL',
'PT',
'RO',
'SM',
'SE',
'CH',
'SK',
'SI',
'ES',
'CZ',
'HU',
'CY')
and type = 'AR'
